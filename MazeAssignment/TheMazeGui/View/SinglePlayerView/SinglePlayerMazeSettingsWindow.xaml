<Window x:Class="TheMazeGui.View.SinglePlayerView.SinglePlayerMazeSettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TheMazeGui.View.SinglePlayerView"
        xmlns:TheUserControl="clr-namespace:TheMazeGui.View.TheUserControl" 
        mc:Ignorable="d"
        Title="SinglePlayerMazeSettingsWindow" Height="210" Width="300">
    <Grid>

        <TheUserControl:SettingsUserControl x:Name="MazeSettingsUC" HorizontalAlignment="Left"  VerticalAlignment="Top"  Loaded="MazeSettingsUC_Loaded" Margin="5,15,0,0" Width="285" Height="110"/>

        <Button Name="okButton" Content="OK" HorizontalAlignment="Left" Margin="112,141,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" Background="MediumSlateBlue"/>

    </Grid>
</Window>
